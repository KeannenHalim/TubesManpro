  <link href="/css/PencarianNama.css" rel="stylesheet">
  <script src="/js/PencarianNama.js" defer></script>
  <title>Pencarian Nama</title>
</head>

<body>
  <%-include('./NavigationBar.ejs') %>
    <div class="group-container">
      <div class="btn-group">
        <button class="btn btn-primary btn-sm" type="button" id="dropdown-container">
          Choose Book
        </button>
        <button type="button" class="btn btn-sm btn-primary dropdown-toggle dropdown-toggle-split"
          data-bs-toggle="dropdown" aria-expanded="false">
          <span class="visually-hidden">Toggle Dropdown</span>
        </button>
        <ul class="dropdown-menu">
          <li><button class="dropdown-item" id="book1">Book 1</button></li>
          <li><button class="dropdown-item" id="book2">Book 2</button></li>
          <li><button class="dropdown-item" id="book3">Book 3</button></li>
          <li><button class="dropdown-item" id="book4">Book 4</button></li>
          <li><button class="dropdown-item" id="book5">Book 5</button></li>
        </ul>
      </div>
      <div class="input-group mb-3" id="input-container">
        <input type="text" class="form-control" id="input-text" aria-label="Text input with dropdown button"
          placeholder="search name">
      </div>
    </div>
    <div class="data-container">
        
    </div>
    <nav aria-label="Page navigation" id="pagination-container">
      <ul class="pagination">
        
      </ul>
    </nav>
